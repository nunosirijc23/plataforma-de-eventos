            <!-- HEADER DESKTOP-->
            <header class="header-desktop2">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap2">
                            <div class="logo d-block d-lg-none">
                                <a href="/users/events">
                                    <!-- <img src="/images/icon/logo-white.png" alt="CoolAdmin" /> -->
                                    <h1 style="color: #FFF;">SPREAD</h1>
                                </a>
                            </div>
                            <div class="header-button2">
                                <% if (isIndex) { %> 
                                <div class="header-button-item mr-0 js-sidebar-btn">
                                    <i class="zmdi zmdi-menu"></i>
                                </div>
                                <div class="setting-menu js-right-sidebar d-none d-lg-block">
                                    <div class="account-dropdown__body">
                                        <div class="account-dropdown__item">
                                            <a href="#" id="btnUpdateUserData" data-toggle="modal" data-target="#updateUserDataModal">
                                                <i class="zmdi zmdi-account"></i>Meus dados
                                            </a>
                                        </div>
                                        <div class="account-dropdown__item">
                                            <a href="#" id="btnChangeUserPhoto" data-toggle="modal" data-target="#changeUserPhotoModal">
                                                <i class="fa fa-picture-o" aria-hidden="true"></i>Alterar foto
                                            </a>
                                        </div>
                                        <div class="account-dropdown__item">
                                            <a href="#" id="btnChangeUserPassword" data-toggle="modal" data-target="#changeUserPasswordModal">
                                                <i class="fa fa-key" aria-hidden="true"></i>Alterar senha
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <% } %> 
                            </div>
                        </div>
                    </div>
                </div>
            </header>

            <aside class="menu-sidebar2 js-right-sidebar d-block d-lg-none">
                <div class="logo">
                    <a href="/users/events">
                        <!-- <img src="/images/icon/logo-white.png" alt="Cool Admin" /> -->
                        <h1 style="color: #FFF;">SPREAD</h1>
                    </a>
                </div>
                <div class="menu-sidebar2__content js-scrollbar2">
                    <div class="account2">
                        <div class="image img-cir img-120">
                            <img style="height: 100%;" src="/images/<%= user.photo %>" alt="<%= user.name %>" />
                        </div>
                        <h4 class="name"><%= user.name %></h4>
                        <a href="/users/logout">Sair</a>
                    </div>
                    <nav class="navbar-sidebar2">
                        <ul class="list-unstyled navbar__list">
                            <% menus.forEach(menu => { %> 
                                <li <% if (menu.active) { %> class="active" <% } %> >
                                    <a class="js-arrow" href="<%= menu.href %>">
                                        <i class="fas fa-<%= menu.icon %> "></i><%= menu.text %> 
                                    </a>
                                </li>
                            <% }) %>
                        </ul>
                    </nav>
                </div>
            </aside>
            <!-- END HEADER DESKTOP-->